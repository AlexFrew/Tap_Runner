<ui:WslDialog x:Class="TAPRunnerLib.UserInteractionWindows.UserInteraction"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TAPRunnerLib.UserInteractionWindows"
             xmlns:ui="clr-namespace:Keysight.Ccl.Wsl.UI;assembly=Keysight.Ccl.Wsl"
        mc:Ignorable="d"
            Height="300" Width="400">
    <Grid>
        <StackPanel>
            <TextBlock x:Name="TitleTextBlock" FontSize="16" FontWeight="Bold" Margin="10" />
            <TextBlock x:Name="MessageTextBlock" Margin="10" />
            <Image x:Name="DialogImage" Height="100" Margin="10" Visibility="Collapsed" />
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Margin="10">
                <Button x:Name="Button1" Width="100" Margin="5" Click="Button_Click" />
                <Button x:Name="Button2" Width="100" Margin="5" Click="Button_Click" />
            </StackPanel>
        </StackPanel>
    </Grid>
</ui:WslDialog>